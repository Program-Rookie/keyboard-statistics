{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "keyboard-statistics",
    "version": "0.1.0",
    "identifier": "com.keyboard-statistics.app",
    "build": {
        "frontendDist": "../src"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [{
            "title": "keyboard-statistics",
            "width": 1200,
            "height": 900,
            "resizable": true,
            "visible": false
        }],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}