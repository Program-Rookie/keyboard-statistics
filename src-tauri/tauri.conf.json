{
    "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/1.0.0/core/tauri-config-schema/schema.json",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devPath": "../src",
        "distDir": "../src"
    },
    "package": {
        "productName": "keyboard-statistics",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            }
        },
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "com.keyboard-statistics.dev",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ]
        },
        "security": {
            "csp": null
        },
        "windows": [{
            "fullscreen": false,
            "resizable": true,
            "title": "键盘统计",
            "width": 1200,
            "height": 800
        }]
    }
}